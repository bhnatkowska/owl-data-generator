<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<generation>
    <scheduler name="DefaultScheduler"/>
    <output name="CSVRowOutput">
        <charset>UTF-8</charset>
        <delimiter>,</delimiter>
        <fileEnding>.csv</fileEnding>
        <fileTemplate>outputDir + table.getName() + fileEnding</fileTemplate>
        <outputDir>/c:/Tmp/datastore/output/test-ontology/</outputDir>
        <quotationCharacter>"</quotationCharacter>
        <quoteStrings>false</quoteStrings>
        <sortByRowID>true</sortByRowID>
    </output>
    <schema name="test-ontology-schema">
        <tables>
            <table name="Company"/>
            <table name="Profession"/>
            <table name="Worker"/>
            <table name="Person"/>
        </tables>
    </schema>
</generation>
